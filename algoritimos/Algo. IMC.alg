Algoritmo "semnome"

Var
M , A , IMC: Real


Inicio
      Escreva ("Massa (Kg) : ")
      Leia (M)
      Escreva ("Altura (m) : ")
      Leia (A)
     IMC <- M / (A ^ 2)
     Escreval ("IMC: " , IMC:5:2)
     Se (IMC >=18.5) e (IMC < 25) Entao
      Escreva ("Parabens voce esta no seu peso ideal")
   Senao
        Escreva (" Lamento mas voce esta gordo")

     FImSe
      


Fimalgoritmo